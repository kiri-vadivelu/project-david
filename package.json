{
  "name": "project-david",
  "version": "1.0.0",
  "description": "Corey David for Scarborough Southwest - Ward 20",
  "main": ".eleventy.js",
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "sass --no-source-map --watch src/scss:_site/css",
    "build:eleventy": "eleventy",
    "build:sass": "sass  --no-source-map --style=compressed src/scss:_site/css",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiri-vadivelu/project-david.git"
  },
  "keywords": [
    "[\"corey-david\"",
    "\"city-council\"",
    "\"scarborough-southwest\"]"
  ],
  "author": "Kiri Vadivelu <kiri@digitaldesigners.ca>",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "github:11ty/eleventy",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.67.0"
  }
}
